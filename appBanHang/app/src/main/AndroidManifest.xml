    package="com.example.appbanhang"
    android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.Order"></activity>
        <activity android:name=".activity.Session" />
        <activity android:name=".activity.xacnhanthongtin" />
        <activity android:name=".activity.Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.UpdateSanpham" />
        <activity android:name=".activity.AddSanPham" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.Quanlysanpham" />
        <activity android:name=".activity.Thongtinkhachhang" />
        <activity android:name=".activity.Giohang" />
        <activity android:name=".activity.ChiTietSanPham" />
        <activity android:name=".activity.DoAnNhanhActivity" />
        <activity android:name=".activity.MonLauActivity" />
        <activity android:name=".activity.MainActivity" />
    </application>

</manifest>